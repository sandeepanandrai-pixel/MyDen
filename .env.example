# MyDen App - Environment Configuration
# Copy this file to .env and fill in your values

# MongoDB Connection
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/myden?retryWrites=true&w=majority

# JWT Secret (generate a strong random string)
JWT_SECRET=change-this-to-a-random-secure-string-minimum-32-characters

# Frontend URL (your EC2 public IP or domain)
FRONTEND_URL=http://YOUR_EC2_IP

# API URL (for frontend to connect to backend)
API_URL=http://YOUR_EC2_IP:5000

# Port Configuration
PORT=5000

# Node Environment
NODE_ENV=production
